{
  "name": "contributte/phpstan",
  "description": "Opinionated set of PHPStan extensions & rules for all fans.",
  "keywords": [
    "phpstan",
    "contributte",
    "rules",
    "extensions",
    "strict"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/phpstan",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "phpstan/phpstan": "^2.0.1",
    "phpstan/phpstan-strict-rules": "^2.0.0",
    "phpstan/phpstan-nette": "^2.0.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Contributte\\Phpstan\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
